<Window x:Class="U2AidenJOlFishingHole.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U2AidenJOlFishingHole"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label 
            Content="Enter how many fish you have caught after each line" 
            HorizontalAlignment="Left" 
            Height="39" 
            Margin="10,10,0,0" 
            VerticalAlignment="Top" 
            Width="497"/>
        <TextBox 
            x:Name="inp" 
            HorizontalAlignment="Left" 
            Height="255" 
            AcceptsReturn="True" 
            Margin="10,54,0,0" 
            TextWrapping="Wrap" 
            Text="TextBox" 
            VerticalAlignment="Top" 
            Width="150"/>
        <Button 
            x:Name="pressbtn"
            Content="Button" 
            HorizontalAlignment="Left" 
            Height="53" 
            Margin="180,129,0,0" 
            VerticalAlignment="Top"
            Width="114"
            Click="Button_Click"/>

    </Grid>
</Window>
